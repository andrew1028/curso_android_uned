<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Ejercicio305">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnWeb"
        android:id="@+id/btnWeb"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="web" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnTel"
        android:id="@+id/btnTel"
        android:layout_below="@+id/btnWeb"
        android:layout_alignRight="@+id/btnWeb"
        android:layout_alignEnd="@+id/btnWeb"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="tel" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnMaps"
        android:id="@+id/btnMaps"
        android:layout_below="@+id/btnTel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/btnTel"
        android:layout_alignEnd="@+id/btnTel"
        android:onClick="maps" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnFoto"
        android:id="@+id/btnFoto"
        android:layout_below="@+id/btnMaps"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/btnMaps"
        android:layout_alignEnd="@+id/btnMaps"
        android:onClick="foto" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnCorreo"
        android:id="@+id/btnCorreo"
        android:layout_below="@+id/btnFoto"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/btnFoto"
        android:layout_alignEnd="@+id/btnFoto"
        android:onClick="correo" />
</RelativeLayout>
